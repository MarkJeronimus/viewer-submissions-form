{
  "name": "decinosubmissionbackend",
  "version": "1.0.0",
  "description": "Nerd",
  "scripts": {
    "tsc": "tsc",
    "build": "npm run barrels && tsc --project tsconfig.compile.json && npm run copy-files",
    "barrels": "barrelsby --config .barrelsby.json",
    "start_js": "node dist/index.js",
    "copy-files": "copyfiles -u 1 src/public/**/* dist/",
    "start": "npm run barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production nodemon dist/index.js",
    "update_deps": "npx npm-check-updates"
  },
  "dependencies": {
    "@tsed/ajv": "^7.9.0",
