    "@tsed/exceptions": "^7.9.0",
    "@tsed/json-mapper": "^7.9.0",
    "@tsed/logger": "^6.3.3",
    "@tsed/logger-file": "^6.3.3",
    "@tsed/passport": "^7.9.0",
    "@tsed/platform-cache": "^7.9.0",
    "@tsed/platform-exceptions": "^7.9.0",
    "@tsed/platform-express": "^7.9.0",
