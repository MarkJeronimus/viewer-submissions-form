    "barrelsby": "^2.5.1",
    "better-sqlite3": "^8.0.1",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "connect-typeorm": "^2.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "dotenv-flow": "^3.2.0",
    "ejs": "^3.1.8",
